
{
  "@context": "http://schema.org",
  "@type": "Article",
  "author": {
    "@type": "Person",
    "@id": "https://nicolasbouliane.com"
  },
  "dateModified": "{{ entry.date_updated.strftime('%F') }}",
  "datePublished": "{{ entry.date_created.strftime('%F') }}",
  "description": "{{ entry.description }}",
  "headline": "{{ entry.title }}",
  "mainEntityOfPage": "{{ entry.url }}",
  "publisher": {
    "@type": "Organization",
    "@id": "{{ site_url }}/#organization"
  }
}

